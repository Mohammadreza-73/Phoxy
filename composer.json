{
    "name": "m.rhaimi/caching_proxy",
    "description": "Caching server that caches responses from other servers.",
    "keywords": ["cache", "caching proxy"],
    "homepage": "https://github.com/Mohammadreza-73/Caching-Proxy-Server",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammadreza",
            "email": "mohammadreza.rahimi1373@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phoxy\\": "src/"
        },
        "files":[
            "src/helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^3.8",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "lint:dry": "./vendor/bin/php-cs-fixer fix --dry-run --diff",
        "lint:diff": "./vendor/bin/php-cs-fixer fix --diff",
        "lint": "./vendor/bin/php-cs-fixer fix",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=1G",
        "test": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
